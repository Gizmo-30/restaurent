footer.footer    
    .footer__about  
        img(src="./images/logo.webp", alt="logo")
        .footer__info 
            ul.messengers
                - var imagesList = ['./images/facebook.svg','./images/instagram.svg','./images/telegram.svg',]
                each items in imagesList 
                    img.messangers-icons(src=items, alt="")
            .footer__menu         
               h3.footer-title CONTACT
               ul
                   -var listItems = ['(302) 555-0107','(239) 555-0108','laperle.com']
                   each items in listItems     
                       li.footer-link=items
            .footer__menu         
               h3.footer-title LOCATION
               p.foote-link   1901 Thornridge Cir. Shiloh, Hawaii 81063
